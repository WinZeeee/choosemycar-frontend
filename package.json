{
  "name": "choosemycar-frontend",
  "private": true,
  "version": "1.0.0",
  "description": "ChooseMyCar Front-End Monorepo - Island Architecture with React 18+, TypeScript, and performance-first principles",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "turbo": "^1.11.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.10.5",
  "keywords": [
    "react",
    "typescript",
    "island-architecture",
    "monorepo",
    "automotive",
    "car-dealership"
  ],
  "author": "ChooseMyCar Team",
  "license": "MIT"
}
